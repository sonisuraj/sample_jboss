                                <td><i class="fa fa-info-circle fa-lg" style="cursor:pointer"></i></td>
                                <td style="font-size:16px">{{sc.name}}</td>
                                <td> <i ng-click="associateDevicesToLD(sc.name)" class="fa fa-plus" style="color: #949394;width:26px;height:26px;line-height:26px;border-radius:100%;text-align:center;border:1px solid #949394;cursor:pointer" /></td>
